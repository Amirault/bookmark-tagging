{
    "name": "Bookmarks Master",
    "version": "0.0.1",
    "description": "package.json",
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "browser_action": {
        "default_title": "Bookmarks Master",
        "default_icon": "./icons/icon.png",
        "default_popup": "index.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
