{
  "name": "Bookmarks Tagging",
  "version": "1.0",
  "description": "Manage bookmarks with tags.",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Bookmarks Tagging",
      "default_icon": "icon.png",
      "default_popup": "index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
